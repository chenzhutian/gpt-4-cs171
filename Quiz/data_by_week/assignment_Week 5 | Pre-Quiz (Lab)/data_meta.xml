&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Multiple">
            <render_choice>
              <response_label ident="53766">
                <material>
                  <mattext texttype="text/plain">50</mattext>
                </material>
              </response_label>
              <response_label ident="3318">
                <material>
                  <mattext texttype="text/plain">500</mattext>
                </material>
              </response_label>
              <response_label ident="72665">
                <material>
                  <mattext texttype="text/plain">100</mattext>
                </material>
              </response_label>
              <response_label ident="32278">
                <material>
                  <mattext texttype="text/plain">1000</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <and>
                <varequal respident="response1">53766</varequal>
                <not>
                  <varequal respident="response1">3318</varequal>
                </not>
                <not>
                  <varequal respident="response1">72665</varequal>
                </not>
                <not>
                  <varequal respident="response1">32278</varequal>
                </not>
              </and>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
