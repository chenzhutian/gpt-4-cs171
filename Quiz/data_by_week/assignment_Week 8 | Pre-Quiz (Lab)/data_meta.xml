      <item ident="gb4eb74a7d3a7520c99e320dcb854d557" title="&quot;this&quot; in JavaScript">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_answers_question</fieldentry>
